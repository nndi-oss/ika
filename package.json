{
  "name": "@nndi/ika",
  "version": "0.1.0",
  "description": "Form filling just got faster, run regular users to power users!",
  "source": "ika.dom.ts",
  "main": "build/ika.js",
  "types": "build/types.d.ts",
  "scripts": {
    "test": "jest",
    "demo": "cp build/* demo/",
    "sass-build": "sass ika.scss build/ika.css",
